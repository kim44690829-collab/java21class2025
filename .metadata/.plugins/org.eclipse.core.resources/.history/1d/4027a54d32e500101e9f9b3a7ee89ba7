package ifExample;

import java.io.IOException;

public class ReadIn {

	public static void main(String[] args) throws IOException {
		// System.in.read() => 키보드로부터 값을 입력받을 수 있다.
		
		int keycode;
		
		keycode = System.in.read();
		System.out.print("값을 입력하세요 : ");
		System.err.println("keycode : " + keycode);

	}

}
